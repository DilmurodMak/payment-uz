# Payment-UZ MCP Server Configuration
# Copy this file to .env and fill in your credentials

# FastMCP Settings
FASTMCP_LOG_LEVEL=INFO
FASTMCP_MASK_ERROR_DETAILS=False

# Payme Configuration (Test Environment)
PAYME_MERCHANT_ID=your_merchant_id_here
PAYME_TEST_KEY=your_test_key_here
PAYME_SECRET_KEY=your_production_key_here

# Click Configuration (Test Environment)
CLICK_SERVICE_ID=your_service_id
CLICK_MERCHANT_ID=your_merchant_id
CLICK_SECRET_KEY=your_secret_key

# Octo Configuration (Test Environment)
OCTO_API_KEY=your_api_key
OCTO_SECRET_KEY=your_secret_key

# Environment
NODE_ENV=development
